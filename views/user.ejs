<!-- main body -->
<main>
  <div  id="loader"class="lds-roller"><div class="center"><div></div><div></div><div></div><div></div><div></div><div></div><div></div><div></div></div></div>
  <div id="main_container" class="">
    <lottie-player id="lottie-player" src="https://assets3.lottiefiles.com/packages/lf20_zfydw4bk.json"
    background="transparent" speed="1" style="width: 500px; height: 350px;" autoplay></lottie-player>

  <h1 id="heading"> WELCOME BACK !<span id="username"><%= name %></span>
  </h1>
  <!-- <span> <%= email %> </span> -->
  <div id="upload-button" class="download_button">
    <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
      style="fill: rgba(0, 0, 0, 1);">
      <path d="M11 15h2V9h3l-4-5-4 5h3z"></path>
      <path d="M20 18H4v-7H2v7c0 1.103.897 2 2 2h16c1.103 0 2-.897 2-2v-7h-2v7z"></path>
    </svg>
  </div>
    
  </div>

  <div id="upload_container" class="invisible">
    <section class="upload-container">
      <form id="uploadForm" enctype="multipart/form-data" method="post">
        <input id="file-input" type="file" name="files" multiple hidden />
        <i class="fas fa-cloud-upload-alt"></i>
        <p>Browse File to Upload</p>
        <!-- <input type="submit" value="upload" name="submit" /> -->
      </form>
       <section class="progress-area"></section>
       <section class="uploaded-area"></section>
     </section>
   
  </div>


  <div id="files_container" class="invisible"> 
    
    
    
  </div>

  
   </main>
   <script src="/static/js/user.js"></script>
   <script src="https://unpkg.com/@lottiefiles/lottie-player@latest/dist/lottie-player.js" defer></script>
